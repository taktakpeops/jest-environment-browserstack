{
  "packages": ["packages/*"],
  "registry": "https://registry.npmjs.org/",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "bootstrap": {
      "ignore": "examples/*"
    },
    "create": {
      "homepage": "https://github.com/taktakpeops/jest-environment-browserstack",
      "license": "MIT"
    },
    "version": {
      "allowBranch": "master",
      "exact": true,
      "message": "chore(release): %s"
    }
  },
  "ignoreChanges": ["examples/**", "**/__tests__/**", "**/__mocks__/**", ".github/**"],
  "version": "2.0.0"
}
